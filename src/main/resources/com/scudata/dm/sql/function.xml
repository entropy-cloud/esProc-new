      <INFO dbtype="TERADATA" value="?1+CAST(?2 AS INTERVAL HOUR)"/>
      <INFO dbtype="POSTGRES" value="?1+interval '?2 hours'"/>
      <INFO dbtype="ESPROC" value="elapse@s(?1,(?2)*3600)"/>
    </FUNCTION>
    <FUNCTION name="ADDMINUTES" paramcount="2" value="">
