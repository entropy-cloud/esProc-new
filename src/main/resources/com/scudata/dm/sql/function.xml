    <FUNCTION name="RIGHT" paramcount="2" value="">
      <INFO dbtype="ORACLE" value="CASE WHEN ?2=0 THEN '' WHEN LENGTH(?1)&gt;?2 THEN SUBSTR(?1,-?2) ELSE ?1 END"/>
