      <INFO dbtype="POSTGRES" value="?1+interval '?2 months'"/>
      <INFO dbtype="ESPROC" value="elapse@m(?1,?2)"/>
    </FUNCTION>
    <FUNCTION name="ADDSECONDS" paramcount="2" value="">
