      <INFO dbtype="HSQL" value="DATEADD('yy', ?2, ?1)"/>
      <INFO dbtype="TERADATA" value="ADD_MONTHS(?1, ?2*12)"/>
      <INFO dbtype="POSTGRES" value="?1+interval '?2 years'"/>
      <INFO dbtype="ESPROC" value="elapse@y(?1,?2)"/>
    </FUNCTION>
